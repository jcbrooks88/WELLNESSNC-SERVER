{
    "name": "server",
    "version": "1.0.0",
    "type": "module",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon --watch src --exec \"node --loader ts-node/esm\" src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "prestart": "npm run build",
        "seed": "ts-node src/seeds/seedDatabase.ts",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist",
    },
    "dependencies": {
        "apollo-server-express": "^3.13.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^2.2.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "graphql": "^16.11.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "mongoose": "^8.13.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^5.0.1",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/lodash.merge": "^4.6.9",
        "@types/node": "^22.14.0",
        "@types/uuid": "^10.0.0",
        "nodemon": "^3.1.9",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
